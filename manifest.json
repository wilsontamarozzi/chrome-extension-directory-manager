{
	"manifest_version": 2,
	"name": "Gerenciador de Diretorio",
	"description": "Gerencie o armazenamento de suas imagens em diretorios especificos",
	"version": "0.3",
	"options_page": "/view/options.html",
	"permissions": [
		"contextMenus", 
		"downloads",
		"storage"
	],
	"background": {
		"persistent": false,
		"scripts": ["/js/script.js"]
	},
	"icons": {
		"16": "/image/icon-16.png",
		"48": "/image/icon-48.png",
		"128": "/image/icon-128.png"
	},
	"browser_action": {
      "default_title": "Gerenciador de Pasta",
      "default_icon": "/image/icon-16.png",
      "default_popup": "/view/options.html"
  }
}